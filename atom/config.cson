      "one-dark-ui"
      "solarized-light-syntax"
    ]
    titleBar: "custom"
  docblockr: {}
  editor:
    fontFamily: "Ubuntu Mono, Source Han Sans"
    invisibles: {}
    preferredLineLength: 140
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    softWrapHangingIndent: 4
    tabLength: 4
    tabType: "soft"
  "exception-reporting":
